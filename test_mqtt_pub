# !/bin/bash
while :
do
    mosquitto_pub -p 1883 -t iot/temperature -m 112322323
    sleep 0.5
    mosquitto_pub -p 1883 -t iot/humidity -m 121212
    sleep 0.5
    mosquitto_pub -p 1883 -t iot/color -m "1|2|3"
    sleep 0.5
    mosquitto_pub -p 1883 -t iot/acc -m "4444|111|222"
    sleep 0.5
    
done